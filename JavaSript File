
function findLargestNumber(numbers) {
    let largest = 0

    for (let i = 0; i < numbers.length; i++) {
        if (numbers[i] > largest) {
            largest = numbers[i]
        }
    }

    return largest
}

console.log(findLargestNumber([-10, -20, -3, -40])) // mong đợi: -3
console.log(findLargestNumber([10, 20, 3, 40])) // mong đợi: -3
